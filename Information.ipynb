    "us_monthly = pd.DataFrame(us_monthly.to_records())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "micperceptions = pd.read_csv(\"data/cleaned/misperception_state_inferred.csv\", index_col=0)\n",
    "micperceptions = pd.DataFrame(micperceptions.to_records())\n",
    "micperceptions.columns = [\"state\", \"pc_misconception\"]\n",
    "micperceptions.pc_misconception = micperceptions.pc_misconception.str.rstrip('%').astype('float') / 100.0"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "us_cross_sectional = us_daily.loc[(us_daily.index >= \"2020-03-01\") & (us_daily.index <= \"2021-01-31\")].groupby([\"state\"]).mean()\n",
    "us_cross_sectional.loc[us_cross_sectional[\"hospitalizedIncrease\"]==0, \"hospitalizedIncrease\"] = np.nan\n",
    "us_Aug = us_daily.loc[(us_daily.index >= \"2020-08-01\") & (us_daily.index <= \"2021-08-31\")].groupby([\"state\"]).mean()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
